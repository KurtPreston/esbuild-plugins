{
  "root": true,
  "extends": "@awesome-code-style",
  "rules": {
    "@typescript-eslint/consistent-type-imports": ["warn", { "prefer": "type-imports" }]
  },
  "overrides": [
    {
      "files": "bundle.js",
      "env": { "node": true },
      "rules": { "@typescript-eslint/no-var-requires": "off" }
    }
  ]
}
